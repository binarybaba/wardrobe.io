    "ecmaVersion": 2016,
    "sourceType": "module"
  },
  "env": {
    "browser": true,
    "node": true
  },
  "plugins":[
    "eslint-plugin-angular"
  ],
  "rules": {
    "angular/controller-as-route":0,
    "angular/no-service-method": 0
  }

}
