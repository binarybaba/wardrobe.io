{
  "presets": [
    "babel-preset-react",
    ["env", {
      "targets": {
        "browsers": "last 2 versions"
      },
      "loose": true,
      "modules": false
    }]
  ],
  "plugins": [
    "babel-plugin-transform-class-properties",
    [
      "babel-plugin-transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ]
  ]
}
